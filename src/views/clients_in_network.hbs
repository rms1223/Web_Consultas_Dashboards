{{>head}}


<link href="cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
<link href="/static/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
<link href="/static/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
<link href="/static/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
<link href="/static/estilos.css" rel="stylesheet">
</head>
{{>body}}
<div class="right_col" role="main">
  <div class="col-md-12 col-sm-12 ">
    <div class="x_panel">
      <div class="x_title">
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="container">
          <div class="col-md-6 col-sm-12 offset-md-3 centered">
            <div class="x_panel">
              <div class="x_title">
                <h2>Buscar Clientes Conectados a la Red CE</h2>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">

                <form class="form-horizontal form-label-left" method="POST" action="/red/clientes/">

                  <div class="form-group row">
                    <label class="col-sm-3 col-form-label">Codigo Presupuestario</label>

                    <div class="col-sm-9">
                      <div class="input-group">
                        <input type="text" class="form-control" name="id">
                        <span class="input-group-btn">
                          <button type="submit" class="btn btn-primary">Buscar</button>
                        </span>
                      </div>
                    </div>
                  </div>
                  <label id="textoCE" class="col-sm-12 col-form-label centered">{{centro_educativo}}</label>
                </form>
              </div>
            </div>
          </div>
        </div>
        <div class="clearfix"></div>
      </div>
      {{#if isListen}}
      <div class="x_content">
        <div class="row">
          <div class="col-sm-12">
            <div class="card-box table-responsive">
              <table id="datatable-buttons" class="table table-striped table-bordered" style="width:100%">
                <thead>
                  <tr>
                    <th>Estado</th>
                    <th>Marca</th>
                    <th>Descripcion</th>
                    <th>Sistema Operativo</th>
                    <th>IP</th>
                    <th>SSID</th>
                    <th>Conectado a</th>
                    <th>Puerto</th>
                    <th>MAC</th>
                  </tr>
                </thead>
                <tbody>
                  {{#clientes}}
                  <tr>
                    {{#if (isOnline status)}}
                    <td class=" text-center"><i class="fa fa-circle" style="color: lightgreen;"></i></td>
                    {{else}}
                    <td class="text-center"><i class="fa fa-circle" style="color: red;"></i></td>
                    {{/if}}
                    {{#if (isData manufacturer description)}}
                    <td>Desconocido</td>
                    <td>Desconocido</td>
                    {{else}}
                    <td>{{manufacturer}}</td>
                    <td>{{description}}</td>
                    {{/if}}                 
                    <td>{{os}}</td>
                    <td>{{ip}}</td>
                    <td>{{ssid}}</td>
                    <td>{{recentDeviceName}}</td>
                    <td>{{switchport}}</td>
                    <td>{{mac}}</td>
                  </tr>
                  {{/clientes}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{else}}
  <h2 class="col-md-12 col-sm-12 offset-md-4">Sin Dispositivos.......</h2>
  {{/if}}
</div>
{{>footer}}
<script src="/static/localrequest.js"></script>

<script src="/static/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
<script src="/static/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
<script src="/static/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
<script src="/static/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
<script src="/static/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
<script src="/static/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
<script src="/static/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
<script src="/static/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
<script src="/static/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
<script src="/static/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
<script src="/static/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
<script src="/static/vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
<script src="/static/vendors/jszip/dist/jszip.min.js"></script>
<script src="/static/vendors/pdfmake/build/pdfmake.min.js"></script>
<script src="/static/vendors/pdfmake/build/vfs_fonts.js"></script>
</div>
</div>
</body>

</html>